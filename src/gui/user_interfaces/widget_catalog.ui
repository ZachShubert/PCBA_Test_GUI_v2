    selection-color: #0f172a;
}

/* Slider */
QSlider::groove:horizontal {
    border: 1px solid #334155;
    height: 6px;
    background-color: #334155;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background-color: #3b82f6;
