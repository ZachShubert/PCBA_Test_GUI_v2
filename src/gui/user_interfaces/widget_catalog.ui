/* ============================================
   LABELS - Text display and status indicators
   ============================================ */

/* Base Label */
QLabel {
    color: #f8fafc;
    background-color: transparent;
    font-size: 13px;
}

/* Heading Labels */
QLabel[class=&quot;heading-1&quot;] {
    font-size: 28px;
    font-weight: bold;
    color: #f8fafc;
}

QLabel[class=&quot;heading-2&quot;] {
    font-size: 24px;
    font-weight: bold;
    color: #f8fafc;
}

QLabel[class=&quot;heading-3&quot;] {
    font-size: 20px;
    font-weight: bold;
    color: #f8fafc;
}

QLabel[class=&quot;heading-4&quot;] {
    font-size: 16px;
    font-weight: bold;
    color: #f8fafc;
}

QLabel[class=&quot;heading-5&quot;] {
    font-size: 14px;
    font-weight: bold;
    color: #f8fafc;
}

/* Text Variants */
QLabel[class=&quot;text-primary&quot;] {
    color: #f8fafc;
}

QLabel[class=&quot;text-secondary&quot;] {
    color: #cbd5e1;
}

QLabel[class=&quot;text-tertiary&quot;] {
    color: #64748b;
}

QLabel[class=&quot;text-muted&quot;] {
    color: #64748b;
    font-size: 12px;
}

QLabel[class=&quot;text-small&quot;] {
    font-size: 11px;
}

QLabel[class=&quot;text-large&quot;] {
    font-size: 15px;
}

QLabel[class=&quot;text-bold&quot;] {
    font-weight: bold;
}

QLabel[class=&quot;text-mono&quot;] {
    font-family: &quot;Consolas&quot;, &quot;Monaco&quot;, &quot;Courier New&quot;, monospace;
}

/* Accent Colors */
QLabel[class=&quot;text-accent-primary&quot;] {
    color: #3b82f6;
}

QLabel[class=&quot;text-accent-secondary&quot;] {
    color: #06b6d4;
}

QLabel[class=&quot;text-success&quot;] {
    color: #22c55e;
}

QLabel[class=&quot;text-warning&quot;] {
    color: #f59e0b;
}

QLabel[class=&quot;text-error&quot;] {
    color: #ef4444;
}

QLabel[class=&quot;text-info&quot;] {
    color: #06b6d4;
}

/* Badge Labels */
QLabel[class=&quot;badge&quot;] {
    background-color: #334155;
    color: #f8fafc;
    border-radius: 12px;
    padding: 4px 12px;
    font-size: 11px;
    font-weight: bold;
}

QLabel[class=&quot;badge-primary&quot;] {
    background-color: #3b82f6;
    color: #0f172a;
    border-radius: 12px;
    padding: 4px 12px;
    font-size: 11px;
    font-weight: bold;
}

QLabel[class=&quot;badge-secondary&quot;] {
    background-color: #06b6d4;
    color: #0f172a;
    border-radius: 12px;
    padding: 4px 12px;
    font-size: 11px;
    font-weight: bold;
}

QLabel[class=&quot;badge-success&quot;] {
    background-color: rgba(34, 197, 94, 0.15);
    color: #22c55e;
    border-radius: 12px;
    padding: 4px 12px;
    font-size: 11px;
    font-weight: bold;
}

QLabel[class=&quot;badge-warning&quot;] {
    background-color: rgba(245, 158, 11, 0.15);
    color: #f59e0b;
    border-radius: 12px;
    padding: 4px 12px;
    font-size: 11px;
    font-weight: bold;
}

QLabel[class=&quot;badge-error&quot;] {
    background-color: rgba(239, 68, 68, 0.15);
    color: #ef4444;
    border-radius: 12px;
    padding: 4px 12px;
    font-size: 11px;
    font-weight: bold;
}

QLabel[class=&quot;badge-info&quot;] {
    background-color: rgba(6, 182, 212, 0.15);
    color: #06b6d4;
    border-radius: 12px;
    padding: 4px 12px;
    font-size: 11px;
    font-weight: bold;
}

/* Pill/Tag Labels */
QLabel[class=&quot;pill&quot;] {
    background-color: #334155;
    color: #f8fafc;
    border-radius: 16px;
    padding: 6px 14px;
    font-size: 12px;
    font-weight: 600;
}

QLabel[class=&quot;pill-outline&quot;] {
    background-color: transparent;
    color: #f8fafc;
    border: 2px solid #334155;
    border-radius: 16px;
    padding: 4px 12px;
    font-size: 12px;
    font-weight: 600;
}

/* Status Indicators */
QLabel[class=&quot;status-online&quot;] {
    color: #22c55e;
    font-weight: bold;
}

QLabel[class=&quot;status-offline&quot;] {
    color: #64748b;
    font-weight: bold;
}

QLabel[class=&quot;status-busy&quot;] {
    color: #ef4444;
    font-weight: bold;
}

QLabel[class=&quot;status-away&quot;] {
    color: #f59e0b;
    font-weight: bold;
}

/* Icon Labels */
QLabel[class=&quot;icon&quot;] {
    font-size: 20px;
    color: #f8fafc;
}

QLabel[class=&quot;icon-sm&quot;] {
    font-size: 16px;
    color: #f8fafc;
}

QLabel[class=&quot;icon-lg&quot;] {
    font-size: 28px;
    color: #f8fafc;
}

QLabel[class=&quot;icon-primary&quot;] {
    color: #3b82f6;
}

QLabel[class=&quot;icon-secondary&quot;] {
    color: #06b6d4;
}

QLabel[class=&quot;icon-success&quot;] {
    color: #22c55e;
}

QLabel[class=&quot;icon-warning&quot;] {
    color: #f59e0b;
}

QLabel[class=&quot;icon-error&quot;] {
    color: #ef4444;
}

QLabel[class=&quot;icon-info&quot;] {
    color: #06b6d4;
}

/* Code/Monospace Labels */
QLabel[class=&quot;code&quot;] {
    font-family: &quot;Consolas&quot;, &quot;Monaco&quot;, &quot;Courier New&quot;, monospace;
    background-color: #334155;
    color: #f8fafc;
    padding: 2px 6px;
    border-radius: 4px;
    font-size: 12px;
}

/* Link-style Labels */
QLabel[class=&quot;link&quot;] {
    color: #3b82f6;
    text-decoration: underline;
}

QLabel[class=&quot;link&quot;]:hover {
    color: #06b6d4;
}

/* Card Title */
QLabel[class=&quot;card-title&quot;] {
    font-size: 16px;
    font-weight: bold;
    color: #f8fafc;
    padding-bottom: 8px;
}

QLabel[class=&quot;card-subtitle&quot;] {
    font-size: 13px;
    color: #cbd5e1;
    padding-bottom: 4px;
}

/* Section Headers */
QLabel[class=&quot;section-header&quot;] {
    font-size: 11px;
    font-weight: bold;
    color: #64748b;
    text-transform: uppercase;
    letter-spacing: 1px;
    padding: 8px 0 4px 0;
}

/* Status Dots for Footer */
QLabel[class=&quot;status-dot-active&quot;] {
    background-color: #22c55e;
    border-radius: 3px;
    min-width: 6px;
    max-width: 6px;
    min-height: 6px;
    max-height: 6px;
}

QLabel[class=&quot;status-dot-inactive&quot;] {
    background-color: #64748b;
    border-radius: 3px;
    min-width: 6px;
    max-width: 6px;
    min-height: 6px;
    max-height: 6px;
}


