    color: #ef4444;
    font-weight: bold;
}

QLabel[class=&quot;status-away&quot;] {
    color: #f59e0b;
    font-weight: bold;
}

/* Icon Labels */
QLabel[class=&quot;icon&quot;] {
    font-size: 20px;
    color: #f8fafc;
}

QLabel[class=&quot;icon-sm&quot;] {
    font-size: 16px;
    color: #f8fafc;
}

QLabel[class=&quot;icon-lg&quot;] {
    font-size: 28px;
    color: #f8fafc;
}

QLabel[class=&quot;icon-primary&quot;] {
    color: #3b82f6;
}

QLabel[class=&quot;icon-secondary&quot;] {
    color: #06b6d4;
}

QLabel[class=&quot;icon-success&quot;] {
    color: #22c55e;
}

QLabel[class=&quot;icon-warning&quot;] {
    color: #f59e0b;
}

QLabel[class=&quot;icon-error&quot;] {
    color: #ef4444;
}

QLabel[class=&quot;icon-info&quot;] {
    color: #06b6d4;
}

/* Code/Monospace Labels */
QLabel[class=&quot;code&quot;] {
    font-family: &quot;Consolas&quot;, &quot;Monaco&quot;, &quot;Courier New&quot;, monospace;
    background-color: #334155;
    color: #f8fafc;
    padding: 2px 6px;
    border-radius: 4px;
    font-size: 12px;
}

/* Link-style Labels */
QLabel[class=&quot;link&quot;] {
    color: #3b82f6;
    text-decoration: underline;
}

